@article{aliyu_correlation_2000-1,
abstract = {Mutual association between characters in pepper (Capsicum annuum) was determined using correlation coefficients from the means of data collected at Samaru, Nigeria during 1991-93. Correlation coefficients were further partitioned into direct and indirect contribution by solving regression equation "path coefficient analysis". There was a positive and highly significant correlation between fresh fruit yield and total dry weight, leaf area index, leaf number and plant height. The association between yield, fruit number, fruit diameter, number of seeds/plant and seed yield was also positive and highly significant. Characters those had a positive and large direct effect on the yield included total dry weight, leaf area index, fruit diameter and number of seeds/plant. Plant height had a negative direct contribution to the final yield. The largest percentage of combined contribution to the yield resulted from the combination of total dry weight and leaf area index, and fruit diameter and number of seeds/plant. Dry biomass, leaf area index, fruit diameter and number of seeds/plant were also greatly involved in the contribution of other characters to fruit yield. An ideotype architecture for pepper should be conical with few but large fruits.},
annote = {Author Affiliation: Department of Agronomy, Anmadu Bello University, Zaria, Nigeria.},
author = {Aliyu, L and Ahmed, M K and Magaji, M D},
journal = {Crop Research},
number = {2},
pages = {318--323},
title = {{Correlation and multiple regression analysis between morphological characters and components of yield in pepper ({\{}Capsicum{\}} annuum {\{}L{\}}.).}},
volume = {19},
year = {2000}
}
