keywords = {Chemical composition,Enzyme activity levels,Pearling and milling,Wheat bran},
pages = {66--72},
title = {{Structure, chemical composition and enzymatic activities of pearlings and bran obtained from pearled wheat ({\{}Triticum{\}} aestivum {\{}L{\}}.) by roller milling}},
