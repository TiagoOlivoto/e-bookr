journal = {Journal of Environmental Sciences},
keywords = {Germination,Physiological activities,Wheat,arsenic},
number = {6},
pages = {725--732},
title = {{Effects of arsenic on seed germination and physiological activities of wheat seedlings}},
url = {http://www.sciencedirect.com/science/article/pii/S1001074207601211},
volume = {19},
year = {2007}
}
