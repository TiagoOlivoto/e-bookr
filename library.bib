keywords = {Biotic interactions,Cerato-platanin,Heavy metals,Phytoalexins,Silene paradoxa},
pages = {38--46},
series = {Plant biotic and abiotic interactions},
shorttitle = {Can adaptation to metalliferous environments affec},
title = {{Can adaptation to metalliferous environments affect plant response to biotic stress? {\{}Insight{\}} from {\{}Silene{\}} paradoxa {\{}L{\}}. and phytoalexins}},
