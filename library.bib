@article{patra_transcriptional_2013-1,
abstract = {Plants produce thousands of secondary metabolites (a.k.a. specialized metabolites) of diverse chemical nature. These compounds play important roles in protecting plants under adverse conditions. Many secondary metabolites are valued for their pharmaceutical properties. Because of their beneficial effects to health, biosynthesis of secondary metabolites has been a prime focus of research. Many transcription factors have been characterized for their roles in regulating biosynthetic pathways at the transcriptional level. The emerging picture of transcriptional regulation of secondary metabolite biosynthesis suggests that the expression of activators and repressors, in response to phytohormones and different environmental signals, forms a dynamic regulatory network that fine-tune the timing, amplitude and tissue specific expression of pathway genes and the subsequent accumulation of these compounds. Recent research has revealed that some metabolic pathways are also controlled by posttranscriptional and posttranslational mechanisms. This review will use recent developments in the biosynthesis of flavonoids, alkaloids and terpenoids to highlight the complexity of transcriptional regulation of secondary metabolite biosynthesis.},
author = {Patra, Barunava and Schluttenhofer, Craig and Wu, Yongmei and Pattanaik, Sitakanta and Yuan, Ling},
doi = {10.1016/j.bbagrm.2013.09.006},
issn = {1874-9399},
journal = {Biochimica et Biophysica Acta (BBA) - Gene Regulatory Mechanisms},
keywords = {Terpenoid indole alkaloids,Transcriptional regulation,flavonoids,terpenes},
number = {11},
pages = {1236--1247},
title = {{Transcriptional regulation of secondary metabolite biosynthesis in plants}},
url = {http://www.sciencedirect.com/science/article/pii/S1874939913001399},
volume = {1829},
year = {2013}
}
