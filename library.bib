}
@article{Kang1991,
abstract = {Integration of stability of performance with yield is essential in yield trials. Methods that select for high yield and stability have been developed, but have not been compared for their usefulness. Our objective was to compare these methods and to study their relationship to yield and the stability-variance statistic ($\sigma$21). We compared Kang's (1988b) rank-sum method (Index 1; equal weights for yield and $\sigma$21) and four additional rank-sum indices [two (Index 2), three (Index 3), four (Index 4), and five (Index 5) times more weight for yield than for stability variance] with H{\"{u}}hn's (1979) S31 and S61 statistics, and Lin and Binns' (1988) P1. All statistics were calculated for each of five sets of data from international maize (Zea mays L.) yield trials. In Set 1, low $\sigma$21's (indicative of stable performance) were generally associated with high yield (r3 = 0.73), but in Set 2 low $\sigma$21's appeared to be associated with low yield (r3 = -0.46). Index 1 ranks were positively correlated with $\sigma$21 ranks in Sets 2 to 5 as were S31 ranks. Index 1 and S31 offered an opportunity to select for both stability based on $\sigma$21 and yield. Indices 2, 3, 4, and 5, and P1 favored selection primarily for yield. It was assumed that the top 50{\%} genotypes would be selected, in Sets 1 and 2, on the basis of yield rank alone or individual statistics. In Set 1, Index 1 was slightly more conservative than S31, in that Index 1 selected a higher yielding genotype from the two lowest yielding genotypes than did S31. S61 was slightly more conservative than Index 1. Index 1 was intermediate between S31 and S61. In Set 2, Index 1 and Index 2 were more conservative than $\sigma$31, whereas $\sigma$61 was more conservative than Index 1, but less conservative than Index 2. P1 favored selection for yield only. We concluded that Kang's rank-sum method (Index 1 here) and H{\"{u}}hn's S31 and S61 statistics would be useful for simultaneously selecting for yield and yield stability. Approved for publication by the Director of the Louisiana Agric. Exp. Stn. as manuscript no. 89-09-3422.},
author = {Kang, M. S. and Pham, H. N.},
doi = {10.2134/agronj1991.00021962008300010037x},
file = {:D$\backslash$:/Documents/Mendeley Desktop/Kang, Pham/Simultaneous Selection for High Yielding and Stable Crop Genotypes - 1991 - Kang, Pham.pdf:pdf},
journal = {Agronomy Journal},
number = {1},
pages = {161},
publisher = {American Society of Agronomy},
title = {{Simultaneous Selection for High Yielding and Stable Crop Genotypes}},
url = {https://dl.sciencesocieties.org/publications/aj/abstracts/83/1/AJ0830010161},
volume = {83},
year = {1991}
}
