journal = {Food Chemistry},
keywords = {Bioactivity,Biosynthesis/bioavailability,Chemical/enzymatic synthesis,Metabolites,Mushrooms,Phenolic acids},
pages = {501--513},
shorttitle = {Bioactivity of phenolic acids},
title = {{Bioactivity of phenolic acids: {\{}Metabolites{\}} versus parent compounds: {\{}A{\}} review}},
