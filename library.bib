keywords = {Lithospermic acid,Lithospermic acid B,Oregano tissue and callus cultures,Proline,Rosmarinic acid,Trade-off between growth and secondary metabolism},
number = {1},
pages = {54--62},
title = {{Relationship of secondary metabolism to growth in oregano ({\{}Origanum{\}} vulgare {\{}L{\}}.) shoot cultures under nutritional stress}},
url = {http://www.sciencedirect.com/science/article/pii/S0098847208001032},
