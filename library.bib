issn = {0304-4238},
journal = {Scientia Horticulturae},
keywords = {Buffering capacity,Fragaria × ananassa,Growing medium,Growth substrate,Water holding capacity},
pages = {217--223},
title = {{Reed canary grass straw as a substrate in soilless cultivation of strawberry}},
url = {http://www.sciencedirect.com/science/article/pii/S0304423814004804},
volume = {178},
year = {2014}
}
