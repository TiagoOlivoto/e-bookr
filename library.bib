@book{li_path_1975,
abstract = {Path coefficients were widely used by the Wright and Lush schools to derive the basic theory of population genetics and animal breeding. They remained a bit of a mystery to other groups, and have now largely fallen into disuse. In this primer, Li gives the best description available of path coefficients and their uses. It is intended for the beginner (the first 100 pages are on simple and multiple regression and correlation) and derives the material in great detail and with much repetition. However, as the author points out, redundancy usually helps the novice to understand relationships better. As a primer, it gives rules and procedures for simple cases, rather than showing general results, or applications to practical problems, or dealing with complications - such as interactions, scale effects or selection. There are 110 pages on population genetics, seeking to interpret its basic results to bio-social scientists. Here it is concerned with details, rather than principles, and may well add to their confusion.Essentially, path coefficients are standardised partial regression coefficients. Path analysis seeks to assign a causal directional model of effects among a series of variables, based on some biological interpretation. However, the model is usually arbitrary, and the path coefficients derived are entirely model-dependent.There are many difficulties in path coefficients that are not considered by Li (because his book is a primer?). There is no discussion of sampling errors of the path coefficients. The analyses are deterministic rather than testing goodness of fit or hypotheses. The methods cannot deal readily with interactions among variables or with selection, and they become very complex when many variables are included. They have largely become outdated by the generality and power of the standard multivariate analysis techniques. The merit in path coefficients may lie in the path diagrams, which help the biologist picture the various causes and interrelationships possible in a complex situation with many variates. A recent use by Rao and Morton [Animal Breeding Abstracts 43, 3176], should be mentioned, in which Wright's path coefficients are integrated with Fisher's tests of hypotheses in analysing familial relationships in man.Li's primer is unlikely to lead to a widespread re-use of path coefficients, but it gives a useful account for anyone interested in their origins and mystery.Charles Smith},
author = {Li, C C},
title = {{Path analysis - a primer.}},
year = {1975}
}
@article{sulmon_abiotic_2015-1,
abstract = {Organisms are regularly subjected to abiotic stressors related to increasing anthropogenic activities, including chemicals and climatic changes that induce major stresses. Based on various key taxa involved in ecosystem functioning (photosynthetic microorganisms, plants, invertebrates), we review how organisms respond and adapt to chemical- and temperature-induced stresses from molecular to population level. Using field-realistic studies, our integrative analysis aims to compare i) how molecular and physiological mechanisms related to protection, repair and energy allocation can impact life history traits of stressed organisms, and ii) to what extent trait responses influence individual and population responses. Common response mechanisms are evident at molecular and cellular scales but become rather difficult to define at higher levels due to evolutionary distance and environmental complexity. We provide new insights into the understanding of the impact of molecular and cellular responses on individual and population dynamics and assess the potential related effects on communities and ecosystem functioning.},
author = {Sulmon, C{\'{e}}cile and van Baaren, Joan and Cabello-Hurtado, Francisco and Gouesbet, Gwenola and Hennion, Fran{\c{c}}oise and Mony, Cendrine and Renault, David and Bormans, Myriam and {El Amrani}, Abdelhak and Wiegand, Claudia and G{\'{e}}rard, Claudia},
doi = {10.1016/j.envpol.2015.03.013},
issn = {0269-7491},
journal = {Environmental Pollution},
keywords = {Chemical and thermal stressors,Common stress responses,Invertebrates,Organization level,Photosynthetic organisms},
pages = {66--77},
shorttitle = {Abiotic stressors and stress responses},
title = {{Abiotic stressors and stress responses: {\{}What{\}} commonalities appear between species across biological organization levels?}},
url = {http://www.sciencedirect.com/science/article/pii/S0269749115001359},
volume = {202},
year = {2015}
}
