@article{coimbra_caracterizacao_2010-2,
abstract = {O objetivo desse trabalho foi caracterizar e avaliar o potencial produtivo e a diverg{\^{e}}ncia gen{\'{e}}tica de popula{\c{c}}{\~{o}}es de milho resgatadas do sudeste de Minas Gerais, visando {\`{a}} identifica{\c{c}}{\~{a}}o de gen{\'{o}}tipos promissores para o melhoramento gen{\'{e}}tico. Foram avaliados quatro h{\'{i}}bridos comerciais, cinco popula{\c{c}}{\~{o}}es de milho crioulo e sete popula{\c{c}}{\~{o}}es de milho de paiol. Foram utilizados 16 descritores, sendo nove quantitativos e sete qualitativos. Para os descritores quantitativos, estudou-se a diverg{\^{e}}ncia gen{\'{e}}tica por meio de an{\'{a}}lise de vari{\'{a}}veis can{\^{o}}nicas. Para os descritores qualitativos, utilizou-se o m{\'{e}}todo de agrupamento de Tocher, tendo como medida de dissimilaridade as vari{\'{a}}veis multicateg{\'{o}}ricas. Houve diverg{\^{e}}ncia gen{\'{e}}tica entre as popula{\c{c}}{\~{o}}es e os descritores quantitativos que mais contribu{\'{i}}ram para a diverg{\^{e}}ncia foram o n{\'{u}}mero de fileiras de gr{\~{a}}os, a largura de gr{\~{a}}os e o di{\^{a}}metro de espiga. Algumas popula{\c{c}}{\~{o}}es podem ter sido cruzadas com h{\'{i}}bridos comerciais nas propriedades rurais e algumas apresentam potencial para serem utilizadas em programas de melhoramento. O agrupamento obtido com a utiliza{\c{c}}{\~{a}}o de dados quantitativos n{\~{a}}o foi o mesmo de quando se utilizou dados qualitativos, por isso, essas informa{\c{c}}{\~{o}}es sobre similaridade devem ser utilizadas de forma conjunta.},
author = {Coimbra, Ronaldo Rodrigues and Miranda, Glauco Vieira and Cruz, Cosme Dami{\~{a}}o and de Melo, Aur{\'{e}}lio Vaz and Eckert, Fernando Roberto},
issn = {1806-6690},
journal = {Revista Ci{\^{e}}ncia Agron{\^{o}}mica},
keywords = {Genetic resources,Landrace,Landraces,Recursos Gen{\'{e}}ticos,Zea mays L},
