journal = {International Journal of Applied Biology and P...},
number = {1},
pages = {257--260},
title = {{{\{}CHARACTER{\}} {\{}ASSOCIATION{\}} {\{}AND{\}} {\{}PATH{\}} {\{}COEFFICIENT{\}} {\{}ANALYSIS{\}} {\{}IN{\}} {\{}MAIZE{\}} ({\{}Zea{\}} mays {\{}L{\}}.)}},
