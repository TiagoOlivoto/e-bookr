@article{aninbon_effect_2016,
abstract = {Terminal drought reduces pod yield and affected the phenolic content of leaves, stems and seed of peanut (Arachis hypogaea L.). The aim of this study was to investigate the effects of end of season water deficit on phenolic content in drought tolerant and sensitive genotypes of peanuts. Five peanut genotypes were planted under two water regimes, field capacity and 1/3 available water. Phenolic content was analyzed in seeds, leaves, and stems. The results revealed that terminal drought decreased phenolic content in seeds of both tolerant and sensitive genotypes. Phenolic content in leaves and stems increased under terminal drought stress in both years. This study provides basic information on changes in phenolic content in several parts of peanut plants when subjected to drought stress. Future studies to define the effect of terminal drought stress on specific phenolic compounds and antioxidant properties in peanut are warranted.},
author = {Aninbon, C and Jogloy, S and Vorasoot, N and Patanothai, A and Nuchadomrong, S and Senawong, T},
