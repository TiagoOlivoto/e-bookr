doi = {10.1016/j.jcs.2005.02.006},
issn = {0733-5210},
journal = {Journal of Cereal Science},
keywords = {1Dx5,Field experiment,GMO,Overexpression},
number = {1},
pages = {15--23},
title = {{Technological quality of transgenic wheat expressing an increased amount of a {\{}HMW{\}} glutenin subunit}},
url = {http://www.sciencedirect.com/science/article/pii/S0733521005000421},
volume = {42},
year = {2005}
}
