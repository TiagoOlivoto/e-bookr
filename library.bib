@article{kramer_wood_2006,
abstract = {In trees, new wood develops from a layer of stem cells called the vascular cambium. A subpopulation of cambial cells—the fusiform initials—are elongated and capable of coordinated reorientation in response to internal and external stimuli. Changes in the orientation of fusiform initials in turn leads to changes in the grain pattern of developing wood. This article reviews the phenomenon of cambial orientation, with an emphasis on a recent computer model that takes the plant hormone auxin as the orienting signal. New model results are presented that demonstrate the surprisingly complex grain patterns that can emerge from simple initial conditions, in qualitative agreement with similar patterns found in wood. Lastly, an alternative theory of wood grain pattern that takes mechanical stress as the orienting signal is critically evaluated.},
author = {Kramer, Eric M},
doi = {10.1007/s00344-006-0065-y},
issn = {0721-7595, 1435-8107},
journal = {Journal of Plant Growth Regulation},
keywords = {Agriculture,Auxin,Cambium,Cell Biology,Forestry,Fusiform initial,Pattern formation,Plant Sciences,Populus,wood grain},
number = {4},
pages = {290--301},
shorttitle = {Wood {\{}Grain{\}} {\{}Pattern{\}} {\{}Formation{\}}},
title = {{Wood {\{}Grain{\}} {\{}Pattern{\}} {\{}Formation{\}}: {\{}A{\}} {\{}Brief{\}} {\{}Review{\}}}},
url = {http://link.springer.com/article/10.1007/s00344-006-0065-y},
volume = {25},
year = {2006}
}
@article{costa_intervalo_2012,
abstract = {A distribui{\c{c}}{\~{a}}o amostral do coeficiente de correla{\c{c}}{\~{a}}o amostral r, sob a hip{\'{o}}tese nula de o coeficiente de correla{\c{c}}{\~{a}}o populacional {\~{n}}=0, {\'{e}} sim{\'{e}}trica, enquanto que, sob a hip{\'{o}}tese nula {\~{n}}≠0, {\'{e}} assim{\'{e}}trica. No primeiro caso, se utiliza uma estat{\'{i}}stica que envolve a distribui{\c{c}}{\~{a}}o t de Student, e no segundo caso, se recorre a uma alternativa desenvolvida por Fisher, a qual d{\'{a}} origem a uma estat{\'{i}}stica com distribui{\c{c}}{\~{a}}o aproximadamente normal, obtida atrav{\'{e}}s da transforma{\c{c}}{\~{a}}o da estat{\'{i}}stica r numa estat{\'{i}}stica {\pounds}, que tem distribui{\c{c}}{\~{a}}o bastante pr{\'{o}}xima da normal. Como alternativa a este {\'{u}}ltimo processo, pode-se criar uma distribui{\c{c}}{\~{a}}o de amostragem real emp{\'{i}}rica de estimativas de coeficientes de correla{\c{c}}{\~{a}}o atrav{\'{e}}s de simula{\c{c}}{\~{o}}es bootstrap e a partir da{\'{i}} construir intervalos de confian{\c{c}}a n{\~{a}}o param{\'{e}}tricos e testar hip{\'{o}}teses para um valor n{\~{a}}o nulo de {\~{n}}, baseada nesta estima{\c{c}}{\~{a}}o intervalar, sem ter que se preocupar com a normalidade da distribui{\c{c}}{\~{a}}o de amostragem da estat{\'{i}}stica r. A proposta deste trabalho {\'{e}} sugerir uma alternativa vi{\'{a}}vel e pr{\'{a}}tica para a constru{\c{c}}{\~{a}}o do intervalo de confian{\c{c}}a para {\~{n}} ou para o teste de signific{\^{a}}ncia de r. De maneira operacional e em tempo h{\'{a}}bil, gra{\c{c}}as ao crescente avan{\c{c}}o da inform{\'{a}}tica e a disponibilidade de variados softwares estat{\'{i}}stico amig{\'{a}}veis, pode ser realizada com mais freq{\"{u}}{\^{e}}ncia a infer{\^{e}}ncia para o coeficiente de correla{\c{c}}{\~{a}}o linear nos casos em que o valor do coeficiente de correla{\c{c}}{\~{a}}o linear populacional testado seja o de n{\~{a}}o nulidade. Portanto, o presente artigo prop{\~{o}}e obter via processo de reamostragem, atrav{\'{e}}s da t{\'{e}}cnica bootstrap, uma distribui{\c{c}}{\~{a}}o por amostragem real emp{\'{i}}rica para a estat{\'{i}}stica r e calcular o seu erro padr{\~{a}}o, possibilitando assim construir intervalo de confian{\c{c}}a e realizar testes de signific{\^{a}}ncia bootstrap para o coeficiente de correla{\c{c}}{\~{a}}o linear referente {\`{a}} hip{\'{o}}tese de um valor n{\~{a}}o nulo.Palavras-chave: coeficiente de correla{\c{c}}{\~{a}}o linear; intervalo de confian{\c{c}}a; teste de signific{\^{a}}ncia; hip{\'{o}}tese nula de coeficiente de correla{\c{c}}{\~{a}}o linear diferente de zero; bootstrap.},
author = {Costa, Giovani Glaucio de Oliveira},
doi = {10.15675/gepros.v0i2.855},
