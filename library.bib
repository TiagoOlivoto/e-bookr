issn = {1364-0321},
journal = {Renewable and Sustainable Energy Reviews},
keywords = {Biodiesel,Oil seeds,TURKEY},
pages = {842--851},
title = {{Biodiesel production potential from oil seeds in {\{}Turkey{\}}}},
url = {http://www.sciencedirect.com/science/article/pii/S1364032115015555},
volume = {58},
year = {2016}
}
