@article{wei_physical_2007-1,
abstract = {Author Summary{\{}{\textless}{\}}p{\{}{\textgreater}{\}}As a cash crop and a model biological system, maize is of great public interest. To facilitate maize molecular breeding and its basic biology research, we built a high-resolution physical map with two different fingerprinting methods on the same set of bacterial artificial chromosome clones. The physical map was integrated to a high-density genetic map and further serves as a framework for the maize genome-sequencing project. Comparative genomics showed that the euchromatic regions between rice and maize are very conserved. Physically we delimited these conserved regions and thus detected many genome rearrangements. We defined extensively the duplication blocks within the maize genome. These blocks allowed us to reconstruct the chromosomes of the maize progenitor. We detected that maize genome has experienced two rounds of genome duplications, an ancient one before maize–rice divergence and a recent one after tetraploidization.{\{}{\textless}{\}}/p{\{}{\textgreater}{\}}{\{}{\textless}{\}}/sec{\{}{\textgreater}{\}}},
author = {Wei, Fusheng and Coe, Ed and Nelson, William and Bharti, Arvind K and Engler, Fred and Butler, Ed and Kim, HyeRan and Goicoechea, Jose Luis and Chen, Mingsheng and Lee, Seunghee and Fuks, Galina and Sanchez-Villeda, Hector and Schroeder, Steven and Fang, Zhiwei and McMullen, Michael and Davis, Georgia and Bowers, John E and Paterson, Andrew H and Schaeffer, Mary and Gardiner, Jack and Cone, Karen and Messing, Joachim and Soderlund, Carol and Wing, Rod A},
doi = {10.1371/journal.pgen.0030123},
journal = {PLoS Genet},
number = {7},
pages = {e123},
title = {{Physical and {\{}Genetic{\}} {\{}Structure{\}} of the {\{}Maize{\}} {\{}Genome{\}} {\{}Reflects{\}} {\{}Its{\}} {\{}Complex{\}} {\{}Evolutionary{\}} {\{}History{\}}}},
url = {http://dx.plos.org/10.1371/journal.pgen.0030123},
volume = {3},
year = {2007}
}
@article{hall_evolution_2013,
abstract = {Diterpene resin acids (DRAs) are major components of pine (Pinus spp.) oleoresin. They play critical roles in conifer defense against insects and pathogens and as a renewable resource for industrial bioproducts. The core structures of DRAs are formed in secondary (i.e. specialized) metabolism via cycloisomerization of geranylgeranyl diphosphate (GGPP) by diterpene synthases (diTPSs). Previously described gymnosperm diTPSs of DRA biosynthesis are bifunctional enzymes that catalyze the initial bicyclization of GGPP followed by rearrangement of a (+)-copalyl diphosphate intermediate at two discrete class II and class I active sites. In contrast, similar diterpenes of gibberellin primary (i.e. general) metabolism are produced by the consecutive activity of two monofunctional class II and class I diTPSs. Using high-throughput transcriptome sequencing, we discovered 11 diTPS from jack pine (Pinus banksiana) and lodgepole pine (Pinus contorta). Three of these were orthologous to known conifer bifunctional levopimaradiene/abietadiene synthases. Surprisingly, two sets of orthologous PbdiTPSs and PcdiTPSs were monofunctional class I enzymes that lacked functional class II active sites and converted (+)-copalyl diphosphate, but not GGPP, into isopimaradiene and pimaradiene as major products. Diterpene profiles and transcriptome sequences of lodgepole pine and jack pine are consistent with roles for these diTPSs in DRA biosynthesis. The monofunctional class I diTPSs of DRA biosynthesis form a new clade within the gymnosperm-specific TPS-d3 subfamily that evolved from bifunctional diTPS rather than monofunctional enzymes (TPS-c and TPS-e) of gibberellin metabolism. Homology modeling suggested alterations in the class I active site that may have contributed to their functional specialization relative to other conifer diTPSs.},
author = {Hall, Dawn E and Zerbe, Philipp and Jancsik, Sharon and Quesada, Alfonso Lara and Dullat, Harpreet and Madilao, Lina L and Yuen, Macaire and Bohlmann, J{\"{o}}rg},
doi = {http://dx.doi.org/10.1104/pp.112.208546},
issn = {, 1532-2548},
journal = {Plant Physiology},
number = {2},
pages = {600--616},
pmid = {23370714},
shorttitle = {Evolution of {\{}Conifer{\}} {\{}Diterpene{\}} {\{}Synthases{\}}},
title = {{Evolution of {\{}Conifer{\}} {\{}Diterpene{\}} {\{}Synthases{\}}: {\{}Diterpene{\}} {\{}Resin{\}} {\{}Acid{\}} {\{}Biosynthesis{\}} in {\{}Lodgepole{\}} {\{}Pine{\}} and {\{}Jack{\}} {\{}Pine{\}} {\{}Involves{\}} {\{}Monofunctional{\}} and {\{}Bifunctional{\}} {\{}Diterpene{\}} {\{}Synthases{\}}}},
url = {http://www.plantphysiol.org/content/161/2/600},
volume = {161},
year = {2013}
}
