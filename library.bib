journal = {LWT - Food Science and Technology},
keywords = {Core browning,MAP,PAL,PPO},
number = {2, Part 2},
pages = {1243--1248},
series = {Food {\{}Innovation{\}} in {\{}China{\}}},
title = {{The effects of modified atmosphere packaging on core browning and the expression patterns of {\{}PPO{\}} and {\{}PAL{\}} genes in ‘{\{}Yali{\}}' pears during cold storage}},
url = {http://www.sciencedirect.com/science/article/pii/S002364381400560X},
volume = {60},
year = {2015}
}
