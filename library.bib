title = {{Gene {\{}Expression{\}} in {\{}Seed{\}} {\{}Development{\}} and {\{}Germination{\}}}},
url = {http://www.sciencedirect.com/science/article/pii/S0079660308607114},
volume = {38},
year = {1990}
}
