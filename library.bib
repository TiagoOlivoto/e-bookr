doi = {10.1626/jcs.59.245},
journal = {Japanese journal of crop science},
keywords = {Dry weight,Heading ratio,Nitrogen absorption ability,Stem elongation stage,Tiller,Total nitrogen content,Wheat},
number = {2},
pages = {245--252},
title = {{Dry {\{}Weight{\}}, {\{}Nitrogen{\}} {\{}Absorption{\}} and {\{}Survival{\}} of {\{}Wheat{\}} {\{}Tillers{\}}}},
volume = {59},
year = {1990}
