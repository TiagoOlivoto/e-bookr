author = {de Souza, Velci Queir{\'{o}}z and Nardino, Maicon and Bonato, Gustavo Orlandi and Bahry, Carlos Andr{\'{e}} and Caron, Braulio Otomar and Zimmer, Paulo Dejalma and Schmidt, Denise},
