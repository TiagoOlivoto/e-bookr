journal = {Small Ruminant Research},
keywords = {Mature weight,Nonlinear models,Parameters},
number = {1–3},
pages = {16--21},
title = {{Growth curves in {\{}Dorper{\}} sheep crossed with the local {\{}Brazilian{\}} breeds, {\{}Morada{\}} {\{}Nova{\}}, {\{}Rabo{\}} {\{}Largo{\}}, and {\{}Santa{\}} {\{}In{\^{e}}s{\}}}},
url = {http://www.sciencedirect.com/science/article/pii/S0921448809000807},
