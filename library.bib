@article{Pereira-Flores2016,
abstract = {Future climatic scenarios can influence crop yield levels and induce hunger if no actions are taken. In this study, we evaluated the effect of increased temperature and CO2 concentration on soybean yield components and biomass partitioning, that ultimately determine the crops productivity. This is conducted for two soybean genotypes that differ in their canopy and life cycles. Experiments were set as follows: a) T1 - ambient temperature and 390 mumolCO2 mol-1, b) T2 - ambient air temperature +2.7 degreesC and ambient CO2, and c) T3 - ambient air temperature + 2.7 degreesC and 750 mumolCO2 mol-1. Results indicate that soybean under elevated CO2 and temperature (T3), were taller and grains weighed more than under the ambient conditions in both cultivars types. However, yield has not increased substantially due to reductions in the ratio of number of branch/plant, pods/branch and grains/branch which lead also to a lesser number of total pods and grains per plant. The increase of temperature favored the number of pods and grains on branches in the modern type cultivar, and on racemes in the old cultivar. This results in more yield in both cultivars versus plants grown in the current ambient, and higher CO2 concentration plus temperature. In both cultivars, the pods and grains partition were higher on the ontogenically oldest four branches and racemes, with decrease in the other upper ones. In addition, the biomass allocation in vegetative tissues was higher than reproductive biomass, with intensity cultivar dependent. We concluded that in future the yield could be limited by reduction of the numbers of branches and racemes in plant, and by alteration at source-sink relation, thus, indicating that changes in canopy architecture are needed to better take advantage of the increasing concentration of CO2.},
author = {Pereira-Flores, Milton E and Justino, Flavio and Ruiz-Vera, Ursula M and Stordal, Frode and Melo, Anderson A Martins and {de Avila Rodrigues}, Rafael},
journal = {Australian Journal of Crop Science},
keywords = {Australasian research information,Australian databases,South-East Asian information,academic research,commissioning,e-titles,education databases,education resources,electronic publisher,full content,information databases,information resources,information sources,library resources,online,online databases,online resources,publishing,research databases,scholarly research},
number = {6},
pages = {808},
publisher = {Southern Cross Publishers},
title = {{Response of soybean yield components and allocation of dry matter to increased temperature and CO2 concentration}},
volume = {10},
year = {2016}
}
