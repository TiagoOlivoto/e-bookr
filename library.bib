file = {::},
issn = {0304-4238},
journal = {Scientia Horticulturae},
month = {mar},
pages = {216--223},
publisher = {Elsevier},
title = {{Phenotypic stability of eggplant for yield and quality through AMMI, GGE and cluster analyses}},
