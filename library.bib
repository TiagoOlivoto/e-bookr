@article{mayer_physical_2012,
abstract = {Barley (Hordeum vulgare L.) is among the world's earliest domesticated and most important crop plants. It is diploid with a large haploid genome of 5.1 gigabases (Gb). Here we present an integrated and ordered physical, genetic and functional sequence resource that describes the barley gene-space in a structured whole-genome context. We developed a physical map of 4.98 Gb, with more than 3.90 Gb anchored to a high-resolution genetic map. Projecting a deep whole-genome shotgun assembly, complementary DNA and deep RNA sequence data onto this framework supports 79,379 transcript clusters, including 26,159 'high-confidence' genes with homology support from other plant genomes. Abundant alternative splicing, premature termination codons and novel transcriptionally active regions suggest that post-transcriptional processing forms an important regulatory layer. Survey sequences from diverse accessions reveal a landscape of extensive single-nucleotide variation. Our data provide a platform for both genome-assisted research and enabling contemporary crop improvement.},
annote = {WOS:000311606000035},
author = {Mayer, Klaus F X and Waugh, Robbie and Langridge, Peter and Close, Timothy J and Wise, Roger P and Graner, Andreas and Matsumoto, Takashi and Sato, Kazuhiro and Schulman, Alan and Muehlbauer, Gary J and Stein, Nils and Ariyadasa, Ruvini and Schulte, Daniela and Poursarebani, Naser and Zhou, Ruonan and Steuernagel, Burkhard and Mascher, Martin and Scholz, Uwe and Shi, Bujun and Langridge, Peter and Madishetty, Kavitha and Svensson, Jan T and Bhat, Prasanna and Moscou, Matthew and Resnik, Josh and Close, Timothy J and Muehlbauer, Gary J and Hedley, Pete and Liu, Hui and Morris, Jenny and Waugh, Robbie and Frenkel, Zeev and Korol, Avraham and Berges, Helene and Graner, Andreas and Stein, Nils and Steuernagel, Burkhard and Taudien, Stefan and Groth, Marco and Felder, Marius and Platzer, Matthias and Brown, John W S and Schulman, Alan and Platzer, Matthias and Fincher, Geoffrey B and Muehlbauer, Gary J and Sato, Kazuhiro and Taudien, Stefan and Sampath, Dharanya and Swarbreck, David and Scalabrin, Simone and Zuccolo, Andrea and Vendramin, Vera and Morgante, Michele and Mayer, Klaus F X and Schulman, Alan},
doi = {10.1038/nature11543},
