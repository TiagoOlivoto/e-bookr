pages = {9--19},
title = {{Assessing the response of yield and comprehensive fruit quality of tomato grown in greenhouse to deficit irrigation and nitrogen application strategies}},
url = {http://www.sciencedirect.com/science/article/pii/S0378377415300561},
volume = {161},
