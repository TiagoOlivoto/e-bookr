journal = {Plant Physiology},
number = {3},
pages = {811--814},
pmid = {16662580},
title = {{Does {\{}Ethylene{\}} {\{}Play{\}} a {\{}Role{\}} in the {\{}Release{\}} of {\{}Lateral{\}} {\{}Buds{\}} ({\{}Tillers{\}}) from {\{}Apical{\}} {\{}Dominance{\}} in {\{}Oats{\}}?}},
