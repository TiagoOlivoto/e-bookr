@article{campos_avaliacao_2010-1,
abstract = {A quantifica{\c{c}}{\~{a}}o da diverg{\^{e}}ncia gen{\'{e}}tica entre acessos de bancos de germoplasma baseada em descritoresempregando-se an{\'{a}}lise multivariada permite a indica{\c{c}}{\~{a}}o de potenciais genitores. Este trabalho teve como objetivoscaracterizar e quantificar a diverg{\^{e}}ncia gen{\'{e}}tica entre 53 acessos de mandioca, por meio de 28 descritores morfol{\'{o}}gicos,submetidos a duas medidas de dissimilaridade e aplicada a dois m{\'{e}}todos de agrupamento, o de Otimiza{\c{c}}{\~{a}}o Tocher e oHier{\'{a}}rquico UPGMA, comparando seus resultados. A caracteriza{\c{c}}{\~{a}}o foi realizada atrav{\'{e}}s de 21 descritoresmorfol{\'{o}}gicos qualitativos e sete descritores morfol{\'{o}}gicos quantitativos, ambos da parte a{\'{e}}rea da planta. O estudopossibilitou verificar os acessos mais divergentes, e tamb{\'{e}}m o comportamento dos dois m{\'{e}}todos de agrupamento, queapresentaram forma{\c{c}}{\~{a}}o de grupos semelhantes, tanto para caracter{\'{i}}sticas qualitativas baseado em vari{\'{a}}veismulticateg{\'{o}}ricas, sendo poss{\'{i}}vel a forma{\c{c}}{\~{a}}o de dez grupos pelo m{\'{e}}todo de Otimiza{\c{c}}{\~{a}}o Tocher e seis grupos peloUPGMA, quanto para caracter{\'{i}}sticas quantitativas, que apresentou forma{\c{c}}{\~{a}}o de dez grupos com uso do Tocher e oitocom UPGMA, baseado na distancia euclidiana. Em ambos os casos o UPGMA complementa o Tocher. O uso conjuntodos dois m{\'{e}}todos associados a an{\'{a}}lises de caracteres qualitativos e quantitativos possibilitou uma avalia{\c{c}}{\~{a}}o mais precisados gen{\'{o}}tipos.},
author = {Campos, Adryellison and Zacarias, Antonio and Costa, Diego and Neves, Leonarda and Barelli, Marco and Sobrinho, Severino and Luz, Petterson},
