title = {{Mapping {\{}QTLs{\}} with epistatic effects and {\{}QTL{\}}×environment interactions by mixed linear model approaches}},
url = {http://link.springer.com/article/10.1007/s001220051331},
volume = {99},
year = {1999}
}
