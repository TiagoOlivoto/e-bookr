journal = {Revista Caatinga},
number = {4},
pages = {190--201},
title = {{{\{}PHYSIOLOGICAL{\}} {\{}QUALITY{\}} {\{}OF{\}} {\{}SOYBEAN{\}} {\{}SEEDS{\}} {\{}UNDER{\}} {\{}MECHANICAL{\}} {\{}INJURIES{\}} {\{}CAUSED{\}} {\{}BY{\}} {\{}COMBINES{\}}}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}abstract{\&}pid=S1983-21252015000400190{\&}lng=en{\&}nrm=iso{\&}tlng=en},
volume = {28},
