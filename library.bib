}
@article{branlard_diversity_1985-1,
abstract = {Research on the relationships between the protein content and the quality characteristics of 70 wheat cultivars was carried out by analysing the high molecular weight (HMW) subunits of glutenin by Polyacrylamide gel electrophoresis in the presence of sodium dodecyl sulphate (SDS-PAGE). The quantity of protein in each of the bands was determined by densitometry. Simple correlations were calculated between the proportion of each of the HMW glutenin subunits and various flour quality characteristics as measured by the Chopin Alveograph which gives strength (W), tenacity (P), swelling (G) and extensibility (L), the Zeleny sedimentation test (Zy) and the Pelshenke swelling test (Pe). All of the characteristics were correlated with at least 4 subunits. Subunits 6 and 8 were the only subunits not correlated significantly with at least one of the characteristics. The parameters W, P, Zy and Pe were correlated positively with bands 5 and 10 and negatively with bands 2 and 12. Subunit 9 was correlated positively with W, P and Zy. Subunits 1 and 2* were correlated positively with G and W, respectively. Elimination of the influence of grain protein content in the variation of the tests from one cultivar to another did not change the sign or significance of the correlations observed. Moreover, if the influence of the heterogeneity of the gliadin bands were eliminated, most of the correlations with tenacity, swelling and extensibility criteria disappeared. Only subunits 17 and 18 remained favourably correlated with swelling. This suggests that gliadins and HMW glutenin subunits interact in producing the rheological characteristics of the dough.},
author = {Branlard, G and Dardevet, M},
doi = {10.1016/S0733-5210(85)80007-2},
issn = {0733-5210},
journal = {Journal of Cereal Science},
number = {4},
pages = {345--354},
shorttitle = {Diversity of grain protein and bread wheat quality},
title = {{Diversity of grain protein and bread wheat quality: {\{}II{\}}. {\{}Correlation{\}} between high molecular weight subunits of glutenin and flour quality characteristics}},
url = {http://www.sciencedirect.com/science/article/pii/S0733521085800072},
volume = {3},
year = {1985}
}
@article{ingram_molecular_1996,
abstract = {Molecular studies of drought stress in plants use a variety of strategies and include different species subjected to a wide range of water deficits. Initial research has by necessity been largely descriptive, and relevant genes have been identified either by reference to physiological evidence or by differential screening. A large number of genes with a potential role in drought tolerance have been described, and major themes in the molecular response have been established. Particular areas of importance are sugar metabolism and late-embryogenesis-abundant (LEA) proteins. Studies have begun to examine mechanisms that control the gene expression, and putative regulatory pathways have been established. Recent attempts to understand gene function have utilized transgenic plants. These efforts are of clear agronomic importance.},
