pmid = {22213811},
title = {{Deposition of ammonium and nitrate in the roots of maize seedlings supplied with different nitrogen salts}},
url = {http://jxb.oxfordjournals.org/content/early/2012/01/02/jxb.err410},
volume = {63},
year = {2012}
}
