number = {2},
pages = {377--384},
shorttitle = {Path analysis of factors influencing household sol},
title = {{Path analysis of factors influencing household solid waste generation: a case study of Xiamen Island, China}},
url = {http://link.springer.com/article/10.1007/s10163-014-0340-0},
volume = {18},
year = {2014}
}
@article{carvalho_alise_2015,
abstract = {O objetivo deste trabalho foi avaliar a estabilidade e a adaptabilidade de oito gen{\'{o}}tipos de cenoura (tr{\^{e}}s cultivares e cinco popula{\c{c}}{\~{o}}es), em 17 ambientes, nas regi{\~{o}}es nordeste, centro-oeste, sudeste e sul do Brasil, nos anos agr{\'{i}}colas 2009/10, 2010/11 e 2011/12. Os experimentos foram conduzidos entre novembro e janeiro de cada ano, conforme o calend{\'{a}}rio agr{\'{i}}cola dos produtores. A parcela experimental foi composta de {\'{a}}rea igual a 1,0 m2 , em delineamento de blocos ao acaso, com tr{\^{e}}s repeti{\c{c}}{\~{o}}es. Os tratos culturais foram similares aos usados pelos produtores. Na colheita, as ra{\'{i}}zes comercializ{\'{a}}veis foram quantificadas e os valores transformados para t ha-1. Realizouse a an{\'{a}}lise de vari{\^{a}}ncia e, posteriormente, a an{\'{a}}lise de fatores e a an{\'{a}}lise de regress{\~{a}}o bissegmentada, visando ao estudo de estabilidade e adaptabilidade. Nenhum gen{\'{o}}tipo apresentou adaptabilidade ampla. A an{\'{a}}lise de fatores conseguiu agrupar menos de 50{\%} dos ambientes avaliados. O gen{\'{o}}tipo ideal n{\~{a}}o foi encontrado pela an{\'{a}}lise de regress{\~{a}}o. O cultivar ‘BRS Planalto' foi o mais adaptado, pelo m{\'{e}}todo da regress{\~{a}}o bissegmentada, enquanto, pela an{\'{a}}lise de fatores, ‘Bras{\'{i}}lia', CNPH-600 e CNPH-589 mostraram-se mais adaptados. Os resultados obtidos por essas metodologias demonstram a dificuldade de recomenda{\c{c}}{\~{a}}o de um cultivar para todas as regi{\~{o}}es produtoras e que programas regionalizados devem ser iniciados. A utiliza{\c{c}}{\~{a}}o dos dois m{\'{e}}todos associados permite a tomada de decis{\~{o}}es mais segura acerca do comportamento dos gen{\'{o}}tipos de cenoura.},
author = {Carvalho, Agnaldo Donizete Ferreira De and da Silva, Giovani Olegario and Pereira, Ricardo Borges and Pinheiro, Jadir Borges},
