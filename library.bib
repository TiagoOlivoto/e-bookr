journal = {Plant Science},
keywords = {Circular dichroism,Desiccation tolerance,Fourier transform infrared spectroscopy,Glycine max physiologically mature proteins,Late embryogenesis abundant,Soybean},
pages = {152--159},
title = {{Functional studies of soybean ({\{}Glycine{\}} max {\{}L{\}}.) seed {\{}LEA{\}} proteins {\{}GmPM{\}}6, {\{}GmPM{\}}11, and {\{}GmPM{\}}30 by {\{}CD{\}} and {\{}FTIR{\}} spectroscopy}},
