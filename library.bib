shorttitle = {Chapter {\{}Eight{\}} - {\{}From{\}} {\{}Bioreactor{\}} to {\{}Entire{\}} },
title = {{Chapter {\{}Eight{\}} - {\{}From{\}} {\{}Bioreactor{\}} to {\{}Entire{\}} {\{}Plants{\}}: {\{}Development{\}} of {\{}Production{\}} {\{}Systems{\}} for {\{}Secondary{\}} {\{}Metabolites{\}}}},
