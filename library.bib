@article{storck_joint_2014-1,
abstract = {O objetivo deste trabalho foi avaliar a contribui{\c{c}}{\~{a}}o do uso de classes de cultivares com e sem intera{\c{c}}{\~{a}}o gen{\'{o}}tipo x ambiente, na qualidade da an{\'{a}}lise conjunta de ensaios de milho, quanto {\`{a}} produtividade de gr{\~{a}}os. Foram usados dados de produtividade de gr{\~{a}}os de milho de 99 ensaios, distribu{\'{i}}dos em 12 grupos, cada um com as mesmas cultivares, em diferentes ambientes. Em cada grupo, 9 a 40 cultivares foram avaliadas em 5 a 12 ambientes, durante tr{\^{e}}s anos agr{\'{i}}colas. Para cada grupo, foi realizada an{\'{a}}lise de vari{\^{a}}ncia conjunta e cada cultivar foi testada quanto a sua contribui{\c{c}}{\~{a}}o para a intera{\c{c}}{\~{a}}o, tendo-se formado duas classes de cultivares: CI, que contribuem para a intera{\c{c}}{\~{a}}o com o ambiente; e SI, que n{\~{a}}o contribuem para a intera{\c{c}}{\~{a}}o com o ambiente. Para cada classe, realizou-se nova an{\'{a}}lise de vari{\^{a}}ncia conjunta e testou-se a contribui{\c{c}}{\~{a}}o da cultivar para a intera{\c{c}}{\~{a}}o. A classifica{\c{c}}{\~{a}}o das cultivares quanto a sua contribui{\c{c}}{\~{a}}o para a intera{\c{c}}{\~{a}}o gen{\'{o}}tipo x ambiente permite realizar an{\'{a}}lise conjunta para cada classe de cultivares, com melhor acur{\'{a}}cia na compara{\c{c}}{\~{a}}o das m{\'{e}}dias das cultivares da classe SI e na an{\'{a}}lise da intera{\c{c}}{\~{a}}o das cultivares da classe CI.Palavras-Chave: Zea mays ; acur{\'{a}}cia seletiva; an{\'{a}}lise de adaptabilidade; indica{\c{c}}{\~{a}}o de cultivares},
author = {Storck, Lindolfo and {Cargnelutti Filho}, Alberto and Guadagnin, Jos{\'{e}} Paulo and Storck, Lindolfo and {Cargnelutti Filho}, Alberto and Guadagnin, Jos{\'{e}} Paulo},
doi = {10.1590/S0100-204X2014000300002},
