title = {{Proteomic {\{}Analysis{\}} of {\{}Silk{\}} {\{}Viability{\}} in {\{}Maize{\}} {\{}Inbred{\}} {\{}Lines{\}} and {\{}Their{\}} {\{}Corresponding{\}} {\{}Hybrids{\}}}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4668103/},
volume = {10},
