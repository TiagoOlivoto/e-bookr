pmid = {12271060},
title = {{Arabidopsis {\{}Flavonoid{\}} {\{}Mutants{\}} {\{}Are{\}} {\{}Hypersensitive{\}} to {\{}UV{\}}-{\{}B{\}} {\{}Irradiation{\}}.}},
