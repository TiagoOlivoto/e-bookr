issn = {0378-4290},
journal = {Field Crops Research},
keywords = {Avena sativa,Grain yield,Lodging,Long days,Main shoot,Oat,Straw length,Tiller,Yield component},
number = {1},
pages = {49--56},
title = {{Seeding rate effects on tillering, grain yield, and yield components of oat at high latitude}},
