publisher = {Embrapa Informa{\c{c}}{\~{a}}o Tecnol{\'{o}}gica},
title = {{Adaptability and stability of white oat cultivars in relation to chemical composition of the caryopsis}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0100-204X2013000100006{\&}lng=en{\&}nrm=iso{\&}tlng=en http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0100-204X2013000100006{\&}lng=en{\&}tlng=en},
