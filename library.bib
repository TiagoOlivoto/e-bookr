@article{mccartney_mapping_2005-2,
abstract = {Relatively little is known about the genetic control of agronomic traits in common wheat ( Triticum aestivum L.) compared with traits that follow Mendelian segregation patterns. A doubled-haploid population was generated from the cross RL4452 × 'AC Domain' to study the inheritance of the agronomic traits: plant height, time to maturity, lodging, grain yield, test weight, and 1000-grain weight. This cross includes the genetics of 2 western Canadian wheat marketing classes. Composite interval mapping was conducted with a microsatellite linkage map, incorporating 369 loci, and phenotypic data from multiple Manitoba environments. The plant height quantitative trait loci (QTLs), QHt.crc-4B and QHt.crc-4D , mapped to the expected locations of Rht-B1 and Rht-D1 . These QTLs were responsible for most of the variation in plant height and were associated with other agronomic traits. An additional 25 agronomic QTLs were detected in the RL4452 × 'AC Domain' population beyond those associated with QHt.crc-4B and QHt.crc-4D . 'AC Domain' contributed 4 alleles for early maturity, including a major time to maturity QTL on 7D. RL4452 contributed 2 major alleles for increased grain yield at QYld.crc-2B and QYld.crc-4A , which are potential targets for marker-assisted selection. A key test weight QTL was detected on 3B and prominent 1000-grain weight QTLs were identified on 3D and 4A. Key words: height, lodging, mapping, maturity, microsatellite markers, test weight, 1000-grain weight, Triticum aestivum , wheat, yield., Relativement peu de choses sont connues au sujet du contrle gntique des caractres agronomiques chez le bl ( Triticum aestivum L.) contrairement au cas des caractres mendliens. Une population d'haplodes doubls a t produite partir du croisement RL4452 'AC Domain' afin d'tudier l'hrdit des caractres suivants : la hauteur, le nombre de jours maturit, la verse, le rendement en grains, le poids volumtrique et le poids de 1000 grains. Ce croisement combine la gntique de 2 bls de l'Ouest Canadien appartenant des classes de mises en march. Une cartographie composite des intervalles a t ralise l'aide d'une carte gntique comptant 369 locus microsatellites et les donnes phnotypiques issues de multiples environnements au Manitoba. Tel qu'attendu, les QTLs de hauteur, QHt.crc-4B et QHt.crc-4D , taient situs dans les rgions contenant Rht-B1 et Rht-D1 . Ces QTLs expliquaient la majorit de la variation en matire de hauteur et ils taient associs d'autres caractres agronomiques. Vingt-cinq autres QTLs agronomiques ont t dcels au sein de la population RL4452 'AC Domain' en plus de QHt.crc-4B et QHt.crc-4D . 'AC Domain' a contribu 4 allles contribuant une maturit htive, incluant un QTL majeur de maturit situ sur 7D. RL4522 a contribu 2 allles majeurs de rendement en grains lesquels taient situs aux locus QYld.crc-2B et QYld.crc-4A et constituent des cibles potentielles pour fins de slection assiste. Un QTL majeur pour le poids volumtrique a t identifi sur 3B et des QTLs importants pour le poids de 1000 grains ont t localiss sur 3D et 4A. Mots cls : hauteur, verse, cartographie, maturit, marqueurs microsatellites, poids volumtrique, poids de 1000 grains, Triticum aestivum , bl, rendement.[Traduit par la Rdaction]},
author = {McCartney, C A and Somers, D J and Humphreys, D G and Lukow, O and Ames, N and Noll, J and Cloutier, S and McCallum, B D},
doi = {10.1139/g05-055},
issn = {0831-2796},
journal = {Genome},
number = {5},
pages = {870--883},
title = {{Mapping quantitative trait loci controlling agronomic traits in the spring wheat cross {\{}RL{\}}4452 '{\{}AC{\}} {\{}Domain{\}}'}},
volume = {48},
year = {2005}
}
@article{arbona_hydrogel_2005-1,
abstract = {Water deficits affect citrus physiology, yield, fruit size and quality. Citrus can respond to drought stress conditions through endogenous hormonal regulation of water status and leaf abscission. In this work, we assayed the efficiency of an amendment to soilless media in delaying the drought stress effect in young citrus seedlings and trees. Substrate amendment promoted plant survival of citrus seedlings subjected to several cycles of drought stress and rehydration. In budded trees, the amendment increased substrate water content, leaf water potential, leaf number, root biomass, CO2 assimilation and stomatal conductance over that of control plants growing in non-amended substrates. We conclude that the substrate amendment reduced the damaging effects of drought stress in citrus plants. The longer survival of seedlings in the amended treatment together with the reduction in leaf abscission and the improvement of physiological parameters, can account for a higher vigour of citrus grown under water stress conditions.},
author = {Arbona, Vicent and Iglesias, Domingo J and Jacas, Josep and Primo-Millo, Eduardo and Talon, Manuel and G{\'{o}}mez-Cadenas, Aurelio},
doi = {10.1007/s11104-004-1160-0},
issn = {0032-079X, 1573-5036},
journal = {Plant and Soil},
keywords = {Ecology,Plant Physiology,Plant Sciences,Soil Science {\&} Conservation,ethylene,hydrogel,leaf abscission,photosynthesis rate,soilless media,stomatal conductance},
number = {1},
pages = {73--82},
title = {{Hydrogel substrate amendment alleviates drought effects on young citrus plants}},
url = {http://link.springer.com/article/10.1007/s11104-004-1160-0},
volume = {270},
year = {2005}
