pages = {353--357},
shorttitle = {Induction of tetraploid poplar and black locust pl},
title = {{Induction of tetraploid poplar and black locust plants using colchicine: chloroplast number as an early marker for selecting polyploids in vitro}},
volume = {99},
year = {2009}
}
