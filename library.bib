@article{torres_correlacao_1999,
abstract = {O objetivo deste trabalho foi comparar a efici{\^{e}}ncia de diferentes testes para determina{\c{c}}{\~{a}}o da qualidade fisiol{\'{o}}gica de sementes de maxixe (Cucumis anguria L.), visando melhorar sua utiliza{\c{c}}{\~{a}}o como testes de qualidade e verificar suas rela{\c{c}}{\~{o}}es com a emerg{\^{e}}ncia de pl{\^{a}}ntulas em campo. Foram avaliadas sementes de sete lotes, pelos testes de germina{\c{c}}{\~{a}}o, primeira contagem de germina{\c{c}}{\~{a}}o, condutividade el{\'{e}}trica a 4 e 24 horas, envelhecimento acelerado, deteriora{\c{c}}{\~{a}}o controlada, teste de frio sem solo e emerg{\^{e}}ncia de pl{\^{a}}ntulas em campo. O trabalho foi desenvolvido no Laborat{\'{o}}rio de An{\'{a}}lise de Sementes e em um campo experimental da Embrapa-CPATSA, em Petrolina, PE, no per{\'{i}}odo de janeiro a maio de 1996. Nos testes de laborat{\'{o}}rio, o delineamento experimental foi inteiramente casualizado, e nos testes de campo foi usado o delineamento em blocos casualizados - ambos com quatro repeti{\c{c}}{\~{o}}es de cinq{\"{u}}enta sementes, sendo as m{\'{e}}dias comparadas pelo teste de Tukey a 1{\%} de probabilidade. Os testes de germina{\c{c}}{\~{a}}o e vigor foram eficientes para detectar diferen{\c{c}}as de qualidade fisiol{\'{o}}gica entre lotes de sementes de maxixe. Os testes de deteriora{\c{c}}{\~{a}}o controlada e de frio sem solo foram mais consistentes na separa{\c{c}}{\~{a}}o dos lotes em diferentes n{\'{i}}veis de vigor, e ao mesmo tempo apresentaram os maiores n{\'{i}}veis de correla{\c{c}}{\~{a}}o com a emerg{\^{e}}ncia de pl{\^{a}}ntulas em campo.},
author = {Torres, Salvador Barros and da Silva, Magn{\'{o}}lia Aparecida Silva and de Carvalho, Ione Maria Soares and de Queir{\'{o}}z, Manoel Ab{\'{i}}lio},
issn = {1678-3921},
journal = {Pesquisa Agropecu{\'{a}}ria Brasileira},
keywords = {$\backslash$textit{\{}Cucumis anguria{\}},physiological quality of seeds,qualidade fisiol{\'{o}}gica de sementes},
