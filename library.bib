@article{Carpes2010,
abstract = {{\textless}p{\textgreater}No planejamento experimental, a variabilidade entre parcelas pode alterar as estimativas dos tamanhos de parcela e de amostra, al{\'{e}}m do n{\'{u}}mero de repeti{\c{c}}{\~{o}}es e defini{\c{c}}{\~{a}}o do delineamento experimental. Para verificar a interfer{\^{e}}ncia da variabilidade das estimativas de valores usados na determina{\c{c}}{\~{a}}o do tamanho de parcelas com diferentes n{\'{u}}meros de plantas e agrupamentos de colheitas, realizou-se um trabalho com dois experimentos. Um experimento foi realizado no inverno/na primavera de 2004, e outro, no ver{\~{a}}o/outono de 2005 com a cultura da abobrinha italiana. As plantas foram cultivadas em t{\'{u}}nel pl{\'{a}}stico, com irriga{\c{c}}{\~{a}}o por gotejamento, com tr{\^{e}}s linhas de cultivo, 24 plantas por linha e espa{\c{c}}amento entre plantas de 0,8m e entre filas de 1,2m. Cada planta foi considerada como a unidade b{\'{a}}sica (UB) do tamanho da parcela. A sequ{\^{e}}ncia de duas, tr{\^{e}}s ou quatro plantas na linha de cultivo formou as parcelas (X). Foram realizadas an{\'{a}}lises considerando-se apenas colheitas individuais e colheitas agrupadas. Para cada experimento, foram obtidas as estimativas da vari{\^{a}}ncia, do coeficiente de varia{\c{c}}{\~{a}}o e do {\'{i}}ndice de informa{\c{c}}{\~{a}}o relativa. Aplicou-se o teste de Bartlett entre as vari{\^{a}}ncias das colheitas individuais e agrupadas em cada tamanho de parcela simulado. Foram verificados acr{\'{e}}scimos nas estimativas da vari{\^{a}}ncia da fitomassa de frutos com o aumento do tamanho da parcela e/ou do n{\'{u}}mero de colheitas agrupadas. As vari{\^{a}}ncias entre as colheitas agrupadas foram homog{\^{e}}neas a partir do agrupamento de tr{\^{e}}s colheitas. O {\'{i}}ndice de informa{\c{c}}{\~{a}}o relativa apresentou valores pr{\'{o}}ximos a 100{\%} com os agrupamentos de colheitas. O arranjo de quatro grupos de colheitas agrupadas, combinado com parcelas de quatro plantas, reduz a variabilidade entre parcelas.{\textless}/p{\textgreater}},
author = {Carpes, Ricardo Howes and L{\'{u}}cio, Alessandro Dal'Col and Lopes, Sidinei Jos{\'{e}} and Benz, Vilson and Haesbaert, Fernando and Santos, Daniel},
