journal = {Annals of Botany},
keywords = {Ceratonia siliquaL.,ammonium,carbohydrate,carob},
number = {6},
pages = {817--823},
title = {{Changes in the {\{}Morphology{\}} of {\{}Roots{\}} and {\{}Leaves{\}} of {\{}Carob{\}} {\{}Seedlings{\}} {\{}Induced{\}} by {\{}Nitrogen{\}} {\{}Source{\}} and {\{}Atmospheric{\}} {\{}Carbon{\}} {\{}Dioxide{\}}}},
