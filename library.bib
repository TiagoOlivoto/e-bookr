pages = {393--408},
title = {{Effects of {\{}Interannual{\}} {\{}Climate{\}} {\{}Variation{\}} on {\{}Aboveground{\}} {\{}Phytomass{\}} in {\{}Alpine{\}} {\{}Vegetation{\}}}},
url = {http://www.esajournals.org/doi/abs/10.2307/1939543},
volume = {75},
year = {1994}
}
