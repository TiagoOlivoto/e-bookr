@article{seki_efeitos_2015,
abstract = {A compacta{\c{c}}{\~{a}}o do solo {\'{e}} o um dos fatores limitante em {\'{a}}reas submetidas ao sistema plantio direto. O manejo utilizado para romper a camada compactada deve mobilizar o m{\'{i}}nimo poss{\'{i}}vel o solo, al{\'{e}}m de manter a cobertura sobre o solo. O objetivo deste trabalho foi avaliar a influ{\^{e}}ncia da subsolagem, da escarifica{\c{c}}{\~{a}}o e da utiliza{\c{c}}{\~{a}}o de mecanismos sulcadores tipo haste na opera{\c{c}}{\~{a}}o de semeadura sobre a manuten{\c{c}}{\~{a}}o da cobertura do solo, o conte{\'{u}}do de {\'{a}}gua e a densidade do solo, assim como os efeitos sobre a produtividade do milho em um Nitossolo Vermelho distrof{\'{e}}rrico h{\'{a}} dez anos cultivado no sistema plantio direto. O delineamento experimental utilizado foi de blocos casualisados, contendo oito tratamentos de manejo do solo: subsolagem a 0,40 m antes da cultura de inverno, subsolagem a 0,40 m antes do milho, escarifica{\c{c}}{\~{a}}o a 0,30 m antes da cultura de inverno, escarifica{\c{c}}{\~{a}}o a 0,30 m antes do milho, escarifica{\c{c}}{\~{a}}o a 0,20 m antes da cultura de inverno, escarifica{\c{c}}{\~{a}}o a 0,20 m antes do milho, plantio direto de milho com mecanismo sulcador tipo haste e plantio direto de milho com mecanismo sulcador tipo disco duplo; e quatro repeti{\c{c}}{\~{o}}es. A subsolagem e a escarifica{\c{c}}{\~{a}}o influenciaram a manuten{\c{c}}{\~{a}}o de cobertura, a densidade e o conte{\'{u}}do de {\'{a}}gua no solo logo ap{\'{o}}s a semeadura, por{\'{e}}m n{\~{a}}o interferiram no desenvolvimento das plantas e na produtividade de gr{\~{a}}os, na cultura do milho. A utiliza{\c{c}}{\~{a}}o de mecanismos sulcadores tipo haste na opera{\c{c}}{\~{a}}o de semeadura n{\~{a}}o apresentou efeito sobre nenhum dos par{\^{a}}metros estudados.},
author = {Seki, Andr{\'{e}} Satoshi and Seki, Fernanda Gon{\c{c}}alves and Jasper, Samir Paulo and Silva, Paulo Roberto Arbex and Benez, S{\'{e}}rgio Hugo},
